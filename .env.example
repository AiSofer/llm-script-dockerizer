# Example .env file for development environment

# LLM vendor (e.g., openai, anthropic, huggingface)
LLM_VENDOR=openai

# OpenAI API key (example, replace with your own)
OPENAI_API_KEY=your_openai_api_key

# OpenAI Model
OPENAI_MODEL=GPT-4o

# Budget for API calls (in USD)
BUDGET=5.0

# Set the environment (development or production)
ENV=development
